{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Booking List",
  "description": "Array of booking IDs returned from GET /booking",
  "type": "array",
  "items": {
    "type": "object",
    "required": ["bookingid"],
    "properties": {
      "bookingid": {
        "type": "integer",
        "minimum": 1
      }
    }
  }
}
